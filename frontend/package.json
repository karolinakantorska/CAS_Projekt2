{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/react-hooks": "^4.0.0",
    "@date-io/date-fns": "^1.3.13",
    "@date-io/dayjs": "^1.3.13",
    "@reduxjs/toolkit": "^1.4.0",
    "@rmwc/avatar": "^6.1.4",
    "@rmwc/button": "^6.1.4",
    "@rmwc/card": "^6.1.4",
    "@rmwc/elevation": "^6.1.4",
    "@rmwc/formfield": "^6.1.4",
    "@rmwc/grid": "^6.1.4",
    "@rmwc/icon": "^6.1.4",
    "@rmwc/icon-button": "^6.1.4",
    "@rmwc/radio": "^6.1.4",
    "@rmwc/ripple": "^6.1.4",
    "@rmwc/select": "^6.1.4",
    "@rmwc/textfield": "^6.1.4",
    "@rmwc/theme": "^6.1.4",
    "@rmwc/typography": "^6.1.4",
    "@testing-library/jest-dom": "^5.11.5",
    "apollo-boost": "^0.4.9",
    "babel-core": "^6.26.3",
    "babel-install": "^2.1.0",
    "date-fns": "^2.16.1",
    "fontsource-hind": "^3.0.9",
    "fontsource-yanone-kaffeesatz": "^3.0.3",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.6.1",
    "jest-transform-graphql": "^2.1.0",
    "next": "9.3.5",
    "next-router": "^1.3.6",
    "next-with-apollo": "^5.1.0",
    "prettier": "^2.1.2",
    "react": "16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "16.13.1",
    "react-router": "^5.2.0",
    "styled-components": "^5.1.1",
    "typeface-hind": "0.0.54"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.6.1",
    "jest-transform-graphql": "^2.1.0",
    "node-fetch": "^2.6.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "babel-plugin-styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
