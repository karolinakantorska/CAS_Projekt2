{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": " jest --watch",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.2",
    "@apollo/react-testing": "^3.1.2",
    "@rmwc/avatar": "^6.1.4",
    "@rmwc/button": "^6.1.4",
    "@rmwc/card": "^6.1.4",
    "@rmwc/checkbox": "^6.1.4",
    "@rmwc/circular-progress": "^6.1.4",
    "@rmwc/dialog": "^6.1.4",
    "@rmwc/elevation": "^6.1.4",
    "@rmwc/formfield": "^6.1.4",
    "@rmwc/grid": "^6.1.4",
    "@rmwc/icon": "^6.1.4",
    "@rmwc/icon-button": "^6.1.4",
    "@rmwc/linear-progress": "^6.1.4",
    "@rmwc/list": "^6.1.4",
    "@rmwc/radio": "^6.1.4",
    "@rmwc/ripple": "^6.1.4",
    "@rmwc/select": "^6.1.4",
    "@rmwc/switch": "^6.1.4",
    "@rmwc/textfield": "^6.1.4",
    "@rmwc/theme": "^6.1.4",
    "@rmwc/typography": "^6.1.4",
    "apollo-boost": "^0.4.9",
    "apollo-upload-client": "^14.1.3",
    "babel-core": "^6.26.3",
    "babel-install": "^2.1.0",
    "babel-jest": "^26.6.3",
    "date-fns": "^2.16.1",
    "eslint-plugin-jest": "^24.1.3",
    "fontsource-hind": "^3.0.9",
    "fontsource-yanone-kaffeesatz": "^3.0.3",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "next": "9.3.5",
    "next-router": "^1.3.6",
    "next-with-apollo": "^5.1.0",
    "prettier": "^2.1.2",
    "react": "16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.1",
    "typeface-hind": "0.0.54"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.6.3",
    "jest-transform-graphql": "^2.1.0",
    "node-fetch": "^2.6.1",
    "waait": "^1.0.5"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "babel-plugin-styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
