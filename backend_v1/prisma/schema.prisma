datasource db {
    provider = "sqlite"
    url = "file:./dev2.db"
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id          Int     @id @default(autoincrement())
    email       String  @unique
    name        String  
    surname     String?
    description String?
    photo String?
    bigPhoto String?
}